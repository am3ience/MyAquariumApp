<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myaquariumapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WaterParams" />
        <activity android:name=".TankTimeline" />
        <activity android:name=".Tasks" />
        <activity android:name=".AddExpense" />
        <activity android:name=".Expenses" />
        <activity android:name=".AddWaterParam" />
        <activity android:name=".AddTimeline" />
        <activity android:name=".AddTask" />
        <activity android:name=".DeleteExpense"></activity>
    </application>

</manifest>